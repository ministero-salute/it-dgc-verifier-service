{ 
    "srcFolders": ["./"],
    "rules": [
        {
            "match": [".*\\.java"],
            "skipIfContains": "Copyright",
            "comments": {
                "start": "/*",
                "line": " *  ",
                "end": "*/"
            },
            "content": ".github/default_license_header.txt"
        }
    ]
}